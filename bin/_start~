#!/bin/bash


# Transfer latest version of script to execute
scp ${_ATOS_PATH}${_UNIT}/${_STARTUP_SCRIPT} ${_REMOTE_USER}@\[${_REMOTE_IP}%eth0\]:${_REMOTE_WORKSPACE}
scp ${_ATOS_PATH}global_settings ${_REMOTE_USER}@\[${_REMOTE_IP}%eth0\]:${_REMOTE_WORKSPACE}
send_tmux "${_REMOTE_WORKSPACE}global_settings" 'Enter'
send_tmux "${_REMOTE_WORKSPACE}${_STARTUP_SCRIPT}" 'Enter'


